/*   1. Faça um programa que determine e mostre os cinco primeiros múltiplos de 3,
      considerando números maiores que 0.  */

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int contagem = 1; // inicialização

    while (contagem <= 5)
    {
        printf("%d \n", 3 * contagem);
        contagem++; // incremento (de um em um)
    }

    system("pause");
    return 0;
}

